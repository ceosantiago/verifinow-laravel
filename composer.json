{
  "name": "verifinow/laravel",
  "description": "Official VerifyNow Laravel package for Age Verification and Identity Verification",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "VerifyNow Team",
      "email": "support@verifinow.io"
    }
  ],
  "keywords": [
    "laravel",
    "verification",
    "identity",
    "age-verification",
    "idv",
    "facial-recognition",
    "verifinow"
  ],
  "homepage": "https://github.com/verifinow/laravel",
  "require": {
    "php": "^8.3",
    "laravel/framework": "^11.0",
    "guzzlehttp/guzzle": "^7.0"
  },
  "require-dev": {
    "pestphp/pest": "^2.0",
    "laravel/pint": "^1.0",
    "phpstan/phpstan": "^1.0",
    "orchestra/testbench": "^9.0"
  },
  "autoload": {
    "psr-4": {
      "VerifyNow\\Laravel\\": "src/"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "VerifyNow\\Laravel\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "VerifyNow\\Laravel\\VerifyNowServiceProvider"
      ],
      "aliases": {
        "VerifyNow": "VerifyNow\\Laravel\\Facades\\VerifyNow",
        "VerifyNowManager": "VerifyNow\\Laravel\\Facades\\VerifyNowManager"
      }
    }
  },
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
